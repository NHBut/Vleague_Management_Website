@using Vleague_Management_Website.Repository;
@model IList<KetQuaThiDauObj>
<div class="cmsmasters_wrap_table">
    <table class="cmsmasters_table">
        <caption></caption>
        <thead>
            <tr class="cmsmasters_table_row_header">
                <th class="cmsmasters_table_cell_alignleft">MATCH DETAILS</th>
                <th class="cmsmasters_table_cell_alignright"></th>
                <th class="cmsmasters_table_cell_alignright"></th>
                <th class="cmsmasters_table_cell_aligncenter"></th>
                <th class="cmsmasters_table_cell_alignleft"></th>
                <th class="cmsmasters_table_cell_alignleft"></th>
                <th class="cmsmasters_table_cell_alignright">LEARN MORE</th>
            </tr>
        </thead>
        @for (var i = 1; i<Model.Count; i++)
        {
            <tr>
                <td class="cmsmasters_table_cell_alignleft">@Model[i].Time</td>
                <td class="cmsmasters_table_cell_alignright">@Model[i].CLB_Nha</td>
                <td class="cmsmasters_table_cell_alignright">@Model[i].BanThangDoiNha</td>
                <td class="cmsmasters_table_cell_aligncenter">-</td>
                <td class="cmsmasters_table_cell_alignleft">@Model[i].BanThangDoiKhach</td>
                <td class="cmsmasters_table_cell_alignleft">@Model[i].CLB_Khach</td>
                <td class="cmsmasters_table_cell_alignright"><a href="../blog/2015/04/15/the-fastet-goal-of-the-world-championship/index.html">Match Report</a></td>
            </tr>
        }
    </table>
</div>